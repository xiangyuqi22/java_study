<assembly>
	<id>dist</id>
	<formats>
		<format>tar.gz</format>
		<format>zip</format>
	</formats>
	<fileSets>
		<fileSet>
			<includes>
				<include>README*</include>
				<include>pom.xml</include>
				<include>doc/*</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>target</directory>
			<outputDirectory></outputDirectory>
			<includes>
				<include>*.jar</include>
			</includes>
		</fileSet>
	</fileSets>
	<!--  -->
	<dependencySets>
		<dependencySet>
			<outputDirectory>/</outputDirectory>
			<useProjectArtifact>false</useProjectArtifact>
			<unpack>false</unpack>
			<scope>runtime</scope>
		</dependencySet>
	</dependencySets>
</assembly>